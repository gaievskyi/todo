{
  "singleQuote": true,
  "trailingComma": "none",
  "arrowParens": "always",
  "bracketSpacing": true,
  "tabWidth": 2,
  "endOfLine": "auto",
  "semi": false,
  "printWidth": 80,
  "importOrder": [
    "^react(.*)$",
    "^\\b(?!react\\b)(.*)$",
    "^@(.*)/?(.*)$",
    "^[./]"
  ],
  "importOrderSortSpecifiers": true
}
